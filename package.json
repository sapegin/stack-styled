{
  "name": "stack-styled",
  "version": "1.0.0",
  "description": "Stacking layouts for React",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/sapegin/stack-styled",
  "repository": "sapegin/stack-styled",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "main": "lib/Stack.js",
  "browser": "esm/Stack.js",
  "files": [
    "lib",
    "esm",
    "emotion"
  ],
  "scripts": {
    "test": "npm run test:jest",
    "posttest": "npm run format",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "styleguidist server",
    "build": "npm run build:esm && npm run build:cjs && npm run build:emotion",
    "build:esm": "babel --delete-dir-on-start --ignore '**/*.spec.js' -d esm/ src/",
    "build:cjs": "babel --delete-dir-on-start --ignore '**/*.spec.js' --env-name cjs -d lib/ src/",
    "build:emotion": "babel --delete-dir-on-start --ignore '**/*.spec.js' --env-name emotion -d emotion/ src/",
    "deploy": "gh-pages -d styleguide-build",
    "docs": "npm run styleguide:build && npm run deploy",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --cache --fix",
    "pretest": "npm run lint",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write '**/*.{js,md}'",
    "precommit": "lint-staged"
  },
  "keywords": [
    "white",
    "space",
    "whitespace",
    "margin",
    "padding",
    "grid",
    "stack",
    "layout",
    "react",
    "component",
    "components",
    "styled-system",
    "styled-components",
    "emotion",
    "design-system",
    "css-in-js"
  ],
  "dependencies": {
    "@styled-system/prop-types": "^5.0.5",
    "styled-system": "^5.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "eslint": "^5.16.0",
    "eslint-config-tamia": "^6.0.2",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-styleguidist": "^9.0.8",
    "react-testing-library": "^6.1.2",
    "styled-components": "^4.2.0",
    "webpack": "^4.30.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/esm/",
      "<rootDir>/emotion/"
    ]
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
